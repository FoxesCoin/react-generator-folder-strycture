{
	"name": "react-file-structure",
	"displayName": "React file structure",
	"publisher": "CyberFox",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.generateReactTemplate",
		"onCommand:extension.generateReactFullTemplate"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.generateReactTemplate",
				"title": "Create React Template"
			},
			{
				"command": "extension.generateReactFullTemplate",
				"title": "Create React Full Template"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.generateReactTemplate",
					"group": "react"
				},
				{
					"command": "extension.generateReactFullTemplate",
					"group": "react"
				}
			]
		}
	},
	"scripts": {
		"package": "rm -rf out && rm -rf react-file-structure-0.0.1.vsix && vsce package",
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"glob": "^7.1.6",
		"vscode-test": "^1.5.0",
		"change-case": "^4.1.2",
		"fs": "^0.0.1-security",
		"fs-extra": "^9.1.0"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.54.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0"
	}
}
